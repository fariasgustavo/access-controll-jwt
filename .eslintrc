{
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": 1,
    "class-methods-use-this": 0,
    "import/extensions": 0,
    "camelcase": [2, { "properties": "never", "ignoreDestructuring": true }],
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-console": [
      "error",
      {
        "allow": ["error"]
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": {
          "multiline": true,
          "minProperties": 5
        },
        "ObjectPattern": {
          "multiline": true,
          "minProperties": 5
        },
        "ImportDeclaration": {
          "multiline": true,
          "minProperties": 5
        },
        "ExportDeclaration": {
          "multiline": true,
          "minProperties": 5
        }
      }
    ]
  }
}
